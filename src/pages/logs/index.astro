---

<Authentication />
<Layout title="Log Reports" route_nav={route_nav}>
	<div class="row">
		<div class="col-lg-12">
			<div class="card-body">
				<div class="pt-3">
					<div class="row justify-content-center">
						<div class="col-xl-12">
							<div>
								<div class="row">
									<div class="col-12">
										<div class="card-body">
											<form>
												<div class="row">
													<div
														class="col d-flex justify-content-end align-items-center"
													>
														<!-- Modifikasi pada div ini -->
														<div class="d-flex flex-column">
															<div class="d-flex">
																<select class="form-control select2">
																	<option> Vendor</option>
																	<option value="CAa">Vendor Satu</option>
																</select>

																<span class="mx-2"></span>

																<input
																	type="text"
																	class="form-control"
																	placeholder="Input RFID, Nomor Lambung ..."
																/>

																<span class="mx-2"></span>

																<div class="d-flex">
																	<button
																		type="button"
																		class="btn btn-primary waves-effect btn-label waves-light me-2"
